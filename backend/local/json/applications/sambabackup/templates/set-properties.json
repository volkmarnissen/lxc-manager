{
  "execute_on": "ve",
  "name": "Set Properties for backup",
  "description": "Sets hostname and defaults.",
  "parameters": [],
  "commands": [
    { "command": "echo { \"id\": \"hostname\", \"value\": \"sambabackup\" }", "outputs": ["hostname"] },
    { "command": "echo { \"id\": \"use_static_ip\", \"value\": true }", "outputs": ["use_static_ip"] },
    { "command": "echo { \"id\": \"ip4_prefix\", \"value\": \"192.168.4\" }", "outputs": ["ip4_prefix"] },
    { "command": "echo { \"id\": \"ip4_cidr\", \"value\": \"24\" }", "outputs": ["ip4_cidr"] },
    { "command": "echo { \"id\": \"ip6_prefix\", \"value\": \"fd00:4::\" }", "outputs": ["ip6_prefix"] },
    { "command": "echo { \"id\": \"ip6_cidr\", \"value\": \"64\" }", "outputs": ["ip6_cidr"] },
    { "command": "echo { \"id\": \"static_gw\", \"value\": \"192.168.4.1\" }", "outputs": ["static_gw"] },
    { "command": "echo { \"id\": \"static_gw6\", \"value\": \"fd00:4::1\" }", "outputs": ["static_gw6"] },
    { "command": "echo { \"id\": \"disk_size\", \"default\": \"1\" }", "outputs": ["disk_size"] }
  ]
}
