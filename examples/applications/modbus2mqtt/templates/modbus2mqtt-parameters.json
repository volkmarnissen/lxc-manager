  "name": "Set Parameters",
  "description": "Set application-specific parameters for Modbus2mqtt",
  "parameters": [],
  "commands": [
    {
      "name": "set-properties",
      "properties": [
        {
          "id": "hostname",
          "value": "modbus2mqtt"
        },
        {
          "id": "oci_image",
          "value": "ghcr.io/modbus2mqtt/modbus2mqtt"
        },
        {
          "id": "uid",
          "value": "0"
        },
        {
          "id": "username",
          "value": "root"
        },
        {
          "id": "volumes",
          "value": "config=/config\ndata=/data\nssl=/ssl"
        }
      ]
    }
  ]
}