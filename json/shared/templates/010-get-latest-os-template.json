{
  "execute_on": "proxmox",
  "name": "Latest OS Template",
  "description": "Download latest operating system template for Proxmox",
  "parameters": [
    {
      "name": "ostype",
      "type": "enum",
      "default": "alpine",
      "description": "Base template to download",
      "enumValues": ["alpine", "debian", "ubuntu"]
    }
  ],
  "outputs": ["template_path"],
  "commands": [
    {
      "type": "script",
      "name": "Latest OS Template",
      "execute": "get-latest-os-template.sh"
    }
  ]
}
